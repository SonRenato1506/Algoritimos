algoritmo "semnome"
// Função :
// Autor :
// Data : 17/03/2023
// Seção de Declarações 
var
contador, limite, salto: inteiro
inicio
Escreval ("Até que número natural deseja contar?")
leia(limite)
Escreval("Qual será o valor do salto?")
Leia(salto)
contador <- 0
Enquanto (contador <= limite) faca
         Escreval(contador)
         contador <- contador + salto
FimEnquanto
fimalgoritmo
